{
  "name": "gatsby-starter-bootstrap",
  "description": "Simple starter for Gatsby",
  "version": "4.0.0",
  "author": "Jack Schumann",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "framer-motion": "^7.6.1",
    "gatsby": "^4.24.3",
    "gatsby-link": "^4.24.1",
    "gatsby-plugin-catch-links": "^4.23.0",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-google-analytics": "^4.3.15",
    "gatsby-plugin-graphql-codegen": "^3.1.0",
    "gatsby-plugin-image": "^2.23.0",
    "gatsby-plugin-manifest": "^4.23.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-offline": "^5.23.0",
    "gatsby-plugin-react-helmet": "^5.23.0",
    "gatsby-plugin-sass": "^5.23.0",
    "gatsby-plugin-sharp": "^4.23.0",
    "gatsby-plugin-sitemap": "^5.23.0",
    "gatsby-plugin-twitter": "^4.23.0",
    "gatsby-plugin-typescript": "^4.23.0",
    "gatsby-remark-copy-linked-files": "^5.23.0",
    "gatsby-remark-images": "^6",
    "gatsby-remark-prismjs": "^6.23.0",
    "gatsby-remark-responsive-iframe": "^5.4.16",
    "gatsby-remark-smartypants": "^5.3.12",
    "gatsby-source-filesystem": "^4.23.0",
    "gatsby-transformer-remark": "^5.23.0",
    "gatsby-transformer-sharp": "^4.23.0",
    "n": "^8.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "animate.css": "4.1.1",
    "babel-eslint": "10.1.0",
    "bootstrap": "^5.2.1",
    "emergence.js": "1.1.2",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "font-awesome": "4.7.0",
    "gh-pages": "3.1.0",
    "husky": "^4.3.8",
    "jquery": "3.5.1",
    "lint-staged": "10.4.0",
    "modern-normalize": "1.0.0",
    "popper.js": "1.16.1",
    "prettier": "2.1.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "react-helmet": "6.1.0",
    "sass": "^1.45.1",
    "textlint": "^12.2.1",
    "textlint-rule-preset-ja-spacing": "2.0.2",
    "textlint-rule-preset-japanese": "5.0.0",
    "typescript": "^4.8.3"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "npm run lint:textfix"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\" --ignore-path .gitignore",
    "lint": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
